{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"kea-typegen watch --write-paths\" -n APP,KEA -c blue,green",
    "build": "kea-typegen write && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "mov2mp4": "cd src/assets && for i in *.mov; do ffmpeg -i \"$i\" -vcodec h264 -acodec mp2 \"${i%.*}.mp4\"; rm \"$i\"; done"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "kea": "^3.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-lazy-load-image-component": "^1.6.0"
  },
  "lint-staged": {
    "*.{png,PNG}": [
      "optipng -clobber -o4 -strip all"
    ],
    "*.{jpg,JPG}": [
      "jpegoptim --strip-all -m76"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-lazy-load-image-component": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "kea-typegen": "^3.1.6",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.4",
    "vite": "^4.4.5"
  }
}
